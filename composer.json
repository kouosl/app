{
    "name": "portalium/portalium-kickstarter",
    "description": "Portalium Project Kickstarter",
    "homepage": "http://www.portalium.dev/",
    "type": "portalium-kickstarter",
    "license": "MIT",
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kouosl/portalium-core.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/kouosl/portalium-theme.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/kouosl/portalium-site.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/kouosl/portalium-user.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "portalium/portalium-core": "dev-master",
        "portalium/portalium-site": "dev-master",
        "portalium/portalium-user": "dev-master",
        "portalium/portalium-theme": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-gii" : "^2.0",
        "yiisoft/yii2-debug": "~2.0.0"
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    },
    "config": {
        "fxp-asset": {
            "enabled": false
        }
    }
}
